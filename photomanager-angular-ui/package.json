{
  "name": "photomanager",
  "version": "0.12.2-SNAPSHOT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "startAngular": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build ",
    "start:local": "ng serve --proxy-config proxy.conf.json --port 19001 --poll=20",
    "test": "ng test anwenderinfo --codeCoverage=true --browsers ChromeHeadless --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "maven_test": "cd .",
    "maven_build": "sync-pom-version && ng build --prod",
    "doc": "compodoc -p src/tsconfig.app.json",
    "sync-pom-version": "sync-pom-version",
    "start:local:no-mock": "ng serve --configuration=local --port 19001",
    "start:local2:no-mock": "ng serve --configuration=local2 --port 19001",
    "start:server:mock": "node server/server.js",
    "start:server:no-mock": "node server/server-nomock.js",
    "start:all:mock": "npm-run-all --parallel start:local start:server:mock",
    "start:all:no-mock": "npm-run-all --parallel start:local:no-mock start:server:no-mock",
    "link:gensuche": "npm link svbaukasten-generische-suche-ui-angular",
